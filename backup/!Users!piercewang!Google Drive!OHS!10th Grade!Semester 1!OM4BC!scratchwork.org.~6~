** Q.005
-(x / sqrt(49 - x^2))
#+BEGIN_SRC python
import re
import sympy
xpoints = [0,55]
equation = "sqrt(x+9)"
solves = []
solved = []
for i in xpoints:
    solves.append(re.sub(r'x',"("+str(i)+")",equation))
for i in solves:
    solved.append(sympy.nsimplify(i))
solved = [str(x) for x in solved]
xpoints = [str(x) for x in xpoints]
slope = sympy.nsimplify(f'({solved[1]}-{solved[0]})/({xpoints[1]} - {xpoints[0]})')
return slope
#+END_SRC

#+RESULTS:
: 1/11
* WP 4.1-4.2
** Q.004
f(x) = cos(x)

:LeftRightendCalc:
#+BEGIN_SRC python
import sympy
import re
def rsf(function, value):
    return re.sub(r'x',value,function)
def determine_area(function, a, b, n):
    change = sympy.nsimplify(f'({b}-{a})/{n}')
    widths = ""
    for i in range(1,int(n)+1):
        i_a = str(i)
        widths = f'{widths}+(' + str(sympy.nsimplify(rsf(function, str(sympy.nsimplify(f'{change}*{i_a}'))))) + ')'
    fullfunction = f'{change}*({widths})'
    return sympy.nsimplify(fullfunction)
return determine_area("10*cos(x)", "0", "pi/2", "10")
#+END_SRC

